{
    "env": {
        "browser": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 2021,
        "sourceType": "module"
    },
    "plugins": [],
    "rules": {
        "react/jsx-uses-react": "warn",   
        "react/jsx-uses-vars": "warn",
        "@typescript-eslint/no-unused-vars": "warn",
        "no-unused-vars": "off"
    }
}
